{
  "name": "rematch-observable",
  "version": "0.1.1",
  "keywords": [
    "redux",
    "rxjs",
    "redux-observable",
    "rematch-observable",
    "rematch"
  ],
  "repository": {
    "url": "https://github.com/JiangWeixian/rematch-observable"
  },
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/rematch-observable.cjs.js",
  "module": "lib/rematch-observable.esm.js",
  "browser": "lib/rematch-observable.umd.js",
  "typings": "src/typings/index.d.ts",
  "scripts": {
    "build": "rimraf lib && rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "pretest": "npm run build",
    "update": "npm update && npm update --save-dev",
    "check": "npm outdated && npm outdated --save-dev",
    "prepublishOnly": "npm run build && np --no-cleanup --yolo --no-publish --any-branch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/**/*.{js,ts,vue,json}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "@rematch/core": ">=1.1.0",
    "redux": ">=4.0.4",
    "rxjs": ">=6.0.0-beta.0 <7"
  },
  "dependencies": {
    "redux-observable": "^1.2.0"
  },
  "devDependencies": {
    "@aiou/eslint-config": "^0.1.4",
    "@changesets/cli": "^2.16.0",
    "@rematch/core": "1.2.0",
    "@types/jest": "24.0.18",
    "eslint": "^7.31.0",
    "husky": "3.0.1",
    "jest": "24.9.0",
    "lint-staged": "^11.1.1",
    "np": "5.0.3",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "rimraf": "3.0.0",
    "rollup": "1.12.2",
    "rollup-plugin-alias": "2.0.0",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-node-resolve": "5.0.0",
    "rollup-plugin-typescript2": "0.24.0",
    "rxjs": "6.5.3",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tslib": "1.9.3",
    "typescript": "3.4.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
